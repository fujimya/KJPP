<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
 >

    <TextView
        android:text="No Order:"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/tv_kode"
        android:layout_alignParentRight="true"
        android:layout_alignParentEnd="true" />

    <TextView
        android:id="@+id/ssd"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/tv_kode"
        android:layout_alignParentStart="true"
        android:layout_alignParentLeft="true"
        android:layout_marginStart="0dp"
        android:layout_marginLeft="0dp"
        android:layout_marginTop="13dp"
        android:text="Nama PT       :"
        android:textColor="@android:color/black"
        android:textStyle="normal|bold" />

    <TextView
        android:id="@+id/textView11"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/ssd"
        android:layout_alignParentStart="true"
        android:layout_alignParentLeft="true"
        android:layout_marginStart="0dp"
        android:layout_marginLeft="0dp"
        android:layout_marginTop="26dp"
        android:text="Lokasi Aset  :"
        android:textColor="@android:color/black"
        android:textStyle="normal|bold" />

    <TextView
        android:id="@+id/tv_lokasiaset"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignStart="@+id/tv_namapt"
        android:layout_alignLeft="@+id/tv_namapt"
        android:layout_alignTop="@+id/textView11"
        android:layout_marginStart="-1dp"
        android:layout_marginLeft="-1dp"
        android:layout_marginTop="-1dp"
        android:text="Jalan Abadi Nusa Tenggara Barat No 15 TBU"
        android:textColor="@android:color/black" />

    <TextView
        android:id="@+id/tv_namapt"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBaseline="@+id/ssd"
        android:layout_marginStart="10dp"
        android:layout_marginLeft="10dp"
        android:layout_toEndOf="@+id/ssd"
        android:layout_toRightOf="@+id/ssd"
        android:text="PT Nusantara Merdeka Abadi"
        android:textColor="@color/colorPrimary"
        android:textStyle="normal|bold" />

    <TextView
        android:id="@+id/tv_koderak"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/textView11"
        android:layout_alignParentStart="true"
        android:layout_alignParentLeft="true"
        android:layout_marginStart="0dp"
        android:layout_marginLeft="0dp"
        android:layout_marginTop="34dp"
        android:text="Tanggal Penawaran      :"
        android:textColor="@android:color/background_dark"
        android:textStyle="normal|bold" />

    <TextView
        android:id="@+id/tv_tanggal"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBaseline="@+id/tv_koderak"
        android:layout_alignStart="@+id/tv_lokasiaset"
        android:layout_alignLeft="@+id/tv_lokasiaset"
        android:layout_alignBottom="@+id/tv_koderak"
        android:layout_marginStart="65dp"
        android:layout_marginLeft="65dp"
        android:layout_marginBottom="0dp"
        android:text="20 - 01 - 2019"
        android:textColor="@color/colorRed" />

    <TextView
        android:id="@+id/textView13"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/tv_koderak"
        android:layout_alignParentStart="true"
        android:layout_alignParentLeft="true"
        android:layout_marginStart="0dp"
        android:layout_marginLeft="0dp"
        android:layout_marginTop="3dp"
        android:text="Fee Penawaran              :"
        android:textColor="@android:color/black"
        android:textStyle="normal|bold" />

    <TextView
        android:id="@+id/textView20"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/textView17"
        android:layout_alignParentStart="true"
        android:layout_alignParentLeft="true"
        android:layout_marginStart="0dp"
        android:layout_marginLeft="0dp"
        android:layout_marginTop="4dp"
        android:text="Due Date                          :"
        android:textColor="@android:color/black"
        android:textStyle="normal|bold" />

    <TextView
        android:id="@+id/tv_duedate"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBaseline="@+id/textView20"
        android:layout_alignStart="@+id/tv_fee"
        android:layout_alignLeft="@+id/tv_fee"
        android:layout_alignBottom="@+id/textView20"
        android:text="20 - 02 - 2019"
        android:textColor="@android:color/holo_green_dark" />

    <TextView
        android:id="@+id/textView22"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBaseline="@+id/tv_properti"
        android:layout_alignBottom="@+id/tv_properti"
        android:layout_alignParentStart="true"
        android:layout_alignParentLeft="true"
        android:text="Nama Properti                :"
        android:textColor="@android:color/black"
        android:textStyle="normal|bold" />

    <TextView
        android:id="@+id/tv_properti"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/tv_duedate"
        android:layout_alignStart="@+id/tv_duedate"
        android:layout_alignLeft="@+id/tv_duedate"
        android:layout_marginTop="12dp"
        android:text="Gedung"
        android:textColor="@android:color/black" />

    <TextView
        android:id="@+id/textView31"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/textView22"
        android:layout_alignParentStart="true"
        android:layout_alignParentLeft="true"
        android:layout_marginTop="13dp"
        android:text="Surveyor Diajukan         :"
        android:textColor="@android:color/black"
        android:textStyle="normal|bold" />

    <TextView
        android:id="@+id/tv_namasurveyor"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignStart="@+id/tv_properti"
        android:layout_alignLeft="@+id/tv_properti"
        android:layout_alignTop="@+id/textView31"
        android:text="Muklis"
        android:textColor="@android:color/black" />

    <TextView
        android:id="@+id/tv_fee"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBaseline="@+id/textView13"
        android:layout_alignStart="@+id/tv_tanggal"
        android:layout_alignLeft="@+id/tv_tanggal"
        android:layout_alignBottom="@+id/textView13"
        android:layout_marginStart="-1dp"
        android:layout_marginLeft="-1dp"
        android:layout_marginBottom="1dp"
        android:text="Rp.24.000,00"
        android:textColor="@color/colorRed" />

    <TextView
        android:id="@+id/textView17"
        android:layout_width="wrap_content"
        android:layout_height="20dp"
        android:layout_below="@+id/textView13"
        android:layout_alignParentStart="true"
        android:layout_alignParentLeft="true"
        android:layout_marginStart="0dp"
        android:layout_marginLeft="0dp"
        android:layout_marginTop="16dp"
        android:text="Harga Deal                      :"
        android:textColor="@android:color/black"
        android:textStyle="normal|bold" />

    <TextView
        android:id="@+id/tv_hargadeal"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignStart="@+id/tv_fee"
        android:layout_alignLeft="@+id/tv_fee"
        android:layout_alignBottom="@+id/textView17"
        android:text="Rp.28.000,00"
        android:textColor="@android:color/holo_green_dark" />

</RelativeLayout>
